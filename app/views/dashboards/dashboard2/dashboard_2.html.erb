<script type="text/javascript">
  $(document).ready(function () {
    $("path").on("click", function () {
      // Reset all path elements
      $("path").attr("transform", ""); // Reset transform
      // Enlarge the clicked path element
      var boundingBox = this.getBBox(); // Get bounding box of clicked path
      var centerX = boundingBox.x + boundingBox.width / 2;
      var centerY = boundingBox.y + boundingBox.height / 2;
      var scaleValue = 1.5;
      var transformValue = "translate(" + (-centerX * (scaleValue - 1)) + "," + (-centerY * (scaleValue - 1)) + ") scale(" + scaleValue + ")";
      $(this).attr("transform", transformValue);
    });
  });
</script>
